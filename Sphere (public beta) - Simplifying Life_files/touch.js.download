function formSubmit() {
    $('.menuForm').submit(function(event) {
        event.preventDefault();
        const startTime = $('#startDropdown').val();
        const endTime = $('#endDropdown').val();
        const startIndex = document.getElementById(startTime);
        const endIndex = document.getElementById(endTime);

        var allCells = $(".time");
        let dragStart = allCells.index(startIndex.closest('.time'));
        let dragEnd = allCells.index(endIndex.closest('.time'));

        $(".time").slice(dragStart, dragEnd+1).find(':checkbox').prop('checked', true);
    
        $('.menuBox').css('display', 'none');
    });
}

